{
  "name": "mvtime",
  "version": "0.1.0",
  "author": "Sander Vonk",
  "description": "A more effective way to manage test schedules, long-term assignments, and exam stress.",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "node deploy.js",
    "postbuild": "cross-env OS_TYPE=$(uname -s) npm-run-all --parallel copy-files echo-message",
    "copy-files": "npm run copy-win || npm run copy-nix",
    "copy-win": "copy /Y dist\\index.html dist\\404.html",
    "copy-nix": "cp dist/index.html dist/404.html",
    "echo-message": "echo 'Copied index.html to 404.html'",
    "publish": "gh-pages -d dist"
  },
  "dependencies": {
    "@lottiefiles/vue-lottie-player": "^1.1.0",
    "@svonk/util": "^1.0.3",
    "apexcharts": "^3.44.0",
    "core-js": "^3.8.3",
    "crypto-js": "^4.1.1",
    "firebase": "^10.1.0",
    "graceful-fs": "^4.2.11",
    "jquery": "^3.6.4",
    "marked": "^5.1.2",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "register-service-worker": "^1.7.2",
    "showdown": "^2.1.0",
    "vue": "^3.2.13",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.0.3",
    "vue-select": "^4.0.0-beta.6",
    "vue-smooth-reflow": "^0.1.12",
    "vue3-apexcharts": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "clean-jsdoc-theme": "^4.2.17",
    "cross-env": "^7.0.3",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gh-pages": "^5.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jsdoc": "^4.0.2",
    "markdown-loader": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "string-replace-loader": "^3.1.0"
  }
}
